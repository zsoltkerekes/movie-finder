{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "Movie Finder",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/icon?family=Material+Icons",
          "https://fonts.gstatic.com/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "https://api.themoviedb.org/**",
        "https://image.tmdb.org/**"
      ],
      "cacheConfig": {
        "maxAge": "1d",
        "maxSize": 600,
        "timeout": "3s",
        "strategy": "performance"
      }
    }
  ]
}
