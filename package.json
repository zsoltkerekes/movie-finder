{
  "name": "movie-finder",
  "version": "0.0.0",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --aot",
    "build": "ng build --prod --aot --build-optimizer --optimization true --common-chunk --vendor-chunk --service-worker --base-href /movie-finder/ --output-path docs --delete-output-path",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update": "npx ng update --all --force && npm audit fix",
    "prettier-check": "prettier --check src",
    "prettier-fix": "prettier --write src",
    "stylelint-check": "stylelint \"src/**/*.scss\"",
    "stylelint-fix": "stylelint \"src/**/*.scss\" --fix",
    "check-all": "npm run prettier-check && npm run prettier-check && npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "9.1.9",
    "@angular/compiler": "9.1.9",
    "@angular/core": "9.1.9",
    "@angular/forms": "9.1.9",
    "@angular/http": "7.2.16",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "9.1.9",
    "@angular/platform-browser-dynamic": "9.1.9",
    "@angular/pwa": "^0.901.7",
    "@angular/router": "9.1.9",
    "@angular/service-worker": "9.1.9",
    "core-js": "^3.6.5",
    "hammerjs": "^2.0.8",
    "ng-lazyload-image": "^7.1.0",
    "ngx-cookie-service": "^3.0.4",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "9.1.9",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.0.5",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "~3.2.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "stylelint": "^13.5.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.2",
    "typescript": "3.8"
  }
}
