{
  "name": "movie-finder",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --aot",
    "build": "ng build --prod --aot --build-optimizer --optimization true --common-chunk --vendor-chunk --service-worker --base-href /movie-finder/ --output-path docs --delete-output-path",
    "test": "ng test --codeCoverage=true --watch=false",
    "lint": "ng lint",
    "eslint": "eslint",
    "e2e": "ng e2e --port 6200",
    "update": "npx ng update --all --force && npm audit fix",
    "prettier-check": "prettier --check src",
    "prettier-fix": "prettier --write src",
    "stylelint-check": "stylelint \"src/**/*.scss\"",
    "stylelint-fix": "stylelint \"src/**/*.scss\" --fix",
    "eslint-check": "eslint src",
    "check-uncommitted-changes": "git diff && git diff-index --quiet HEAD -- || exit 1",
    "eslint-fix": "eslint src --fix",
    "fix-all": "npm run stylelint-fix && npm run prettier-fix && npm run eslint-fix",
    "check-all": "npm run prettier-check && npm run stylelint-check && npm run lint && npm run eslint-check && npm run test && npm run e2e",
    "sonar": "sonar-scanner",
    "deploy": "npm run check-all && npm run check-uncommitted-changes && npm run build && git add . && git commit -m \"new build\" && git push --no-verify"
  },
  "engines": {
    "node": ">=12 <13"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "9.1.9",
    "@angular/compiler": "9.1.9",
    "@angular/core": "9.1.9",
    "@angular/forms": "9.1.9",
    "@angular/http": "7.2.16",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "9.1.9",
    "@angular/platform-browser-dynamic": "9.1.9",
    "@angular/pwa": "^0.901.12",
    "@angular/router": "9.1.9",
    "@angular/service-worker": "9.1.9",
    "core-js": "^3.6.5",
    "hammerjs": "^2.0.8",
    "ng-lazyload-image": "^8.0.1",
    "ngx-cookie-service": "^3.1.3",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "9.1.9",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "codelyzer": "^5.2.2",
    "eslint": "^7.10.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "sonarqube-scanner": "^2.7.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~8.10.2",
    "tslint": "^6.1.3",
    "typescript": "3.8"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run check-all"
    }
  },
  "recommendations": [
    "angular.ng-template",
    "editorconfig.editorconfig",
    "christian-kohler.path-intellisense",
    "esbenp.prettier-vscode",
    "stylelint.vscode-stylelint",
    "ms-vscode.vscode-typescript-tslint-plugin",
    "eg2.vscode-npm-script",
    "christian-kohler.npm-intellisense",
    "eamodio.gitlens",
    "waderyan.gitblame",
    "streetsidesoftware.code-spell-checker",
    "sonarsource.sonarlint-vscode",
    "ryanluker.vscode-coverage-gutters"
  ]
}
